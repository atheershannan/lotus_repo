{
  "version": 2,
  "public": false,
  "buildCommand": "echo 'Skipping build - serving static CHATBOT.html'",
  "outputDirectory": ".",
  "rewrites": [
    {
      "source": "/(.*)",
      "destination": "/CHATBOT.html"
    }
  ],
  "headers": [
    {
      "source": "/CHATBOT.html",
      "headers": [
        {
          "key": "Cache-Control",
          "value": "public, max-age=3600"
        }
      ]
    }
  ]
}

