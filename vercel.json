{
  "buildCommand": "cd FRONTEND && npm install && npm run build",
  "outputDirectory": "FRONTEND/build",
  "framework": "create-react-app",
  "rewrites": [
    { 
      "source": "/((?!api/.*|chatbot.html).*)", 
      "destination": "/index.html" 
    }
  ],
  "headers": [
    {
      "source": "/static/(.*)",
      "headers": [
        {
          "key": "Cache-Control",
          "value": "public, max-age=31536000, immutable"
        }
      ]
    }
  ]
}
